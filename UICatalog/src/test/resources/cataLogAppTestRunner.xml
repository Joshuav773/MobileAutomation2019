<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="iOS test suite on iPhone Simulator">

    <parameter name="OS" value="ios"/>
    <parameter name="appType" value="iPhone"/>
    <parameter name="deviceType" value="Simulator"/>
    <parameter name="deviceName" value="iPhone Simulator"/>
    <parameter name="version" value="12.1"/>

    <test name="Go to Home page" preserve-order="true">
        <classes>
            <class name="UICatalogTests.ButtonsTest"></class>
            <class name="UICatalogTests.AlertTest"></class>
            <class name="UICatalogTests.ControlsTests"></class>
            <class name="UICatalogTests.ImageTest"></class>
            <class name="UICatalogTests.UITest"></class>
            <class name="UICatalogTests.PickersTest"></class>
            <class name="UICatalogTests.SegmentsTests"></class>
            <class name="UICatalogTests.TextFieldsTests"></class>
            <class name="UICatalogTests.TextViewButtonTest"></class>
            <class name="UICatalogTests.ToolBarTests"></class>
            <class name="UICatalogTests.WebTest"></class>
        </classes>
    </test>
</suite>
